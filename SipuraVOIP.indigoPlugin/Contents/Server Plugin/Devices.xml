</ConfigUI>

    <States>
      <State id="deviceOnline">
        <ValueType>Boolean</ValueType>
        <TriggerLabel>Device online?</TriggerLabel>
        <ControlPageLabel>Device online?</ControlPageLabel>
      </State>

        <State id="deviceStatus">
        <ValueType>String</ValueType>
        <TriggerLabel>Device Status</TriggerLabel>
        <ControlPageLabel>Device Status</ControlPageLabel>
      </State>
      <State id="deviceLastUpdated">
        <ValueType>String</ValueType>
        <TriggerLabel>Device Last Updated</TriggerLabel>
        <ControlPageLabel>Device Last Updated</ControlPageLabel>
      </State>

      <State id="lastNumber">
        <ValueType>String</ValueType>
        <TriggerLabel>Number of Phone</TriggerLabel>
        <ControlPageLabel>Number of Phone</ControlPageLabel>
      </State>
      <State id="currentNumber">
        <ValueType>String</ValueType>
        <TriggerLabel>Current Number of Phone</TriggerLabel>
        <ControlPageLabel>Current Number of Phone</ControlPageLabel>
      </State>
        <State id="callType">
        <ValueType>String</ValueType>
        <TriggerLabel>Type of Call</TriggerLabel>
        <ControlPageLabel>Type of Call</ControlPageLabel>
      </State>

      <State id="callTime">
        <ValueType>Number</ValueType>
        <TriggerLabel>Time of Last Call</TriggerLabel>
        <ControlPageLabel>Time of Last Call</ControlPageLabel>
      </State>

      <State id="durationCall">
        <ValueType>String</ValueType>
        <TriggerLabel>Duration of Last Call</TriggerLabel>
        <ControlPageLabel>Duration of Last Call</ControlPageLabel>
      </State>


    </States>

    <UiDisplayStateId>deviceStatus</UiDisplayStateId>




