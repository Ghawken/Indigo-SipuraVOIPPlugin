        <Label>VOIP IP Address:</Label>
      </Field>      

       <Field id="label4" type="label" fontSize="small" fontColor="black">
        <Label> </Label>
      </Field>

       <Field id="sep1" type="separator"/>

      
      <Field id="sourceXML" type="textfield">
        <Label>IP Address</Label>

      </Field>
      <Field id="example13" type="label" fontSize="small" fontColor="black" alignText="right" >
        <Label> Spirua IP - will need to enable static IP address in DHCP Server</Label>
      </Field>

         <Field id="sep2" type="separator"/>

      <Field id="sourcePort" type="textfield">
        <Label>Port :</Label>
      </Field>

      <Field id="example3" type="label" fontSize="small" fontColor="black" alignText="right" >
        <Label> Spirua Port - as configured in Spirua System Settings</Label>
      </Field>

         <Field id="sep4" type="separator"/>

      <Field id="sipServer" type="textfield">
